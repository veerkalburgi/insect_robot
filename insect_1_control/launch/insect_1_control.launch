<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <rosparam file="$(find insect_1_control)/config/insect_rbot_control.yaml" command="load"/>


    <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" ns="/insect" args="l_f_hip_joint_position_controller r_f_hip_joint_position_controller l_b_hip_joint_position_controller r_b_hip_joint_position_controller l_f_leg_joint_position_controller r_f_leg_joint_position_controller l_b_leg_joint_position_controller r_b_leg_joint_position_controller joint_state_controller"/>
    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"
      respawn="false" output="screen">
      <remap from="/joint_states" to="/insect/joint_states" />
    </node>
</launch>
