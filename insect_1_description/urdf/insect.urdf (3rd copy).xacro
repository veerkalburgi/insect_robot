<xacro:property name="connect_offset" value="0.02633" />


<!--#############################################################################################################################-->
<!--macro param-->
<xacro:macro name="default_inertial" params="mass">
   <inertial>
     <origin xyz="0 -${connect_offset} 0" rpy="0 0 0" />
     <mass value="${mass}" />
     <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
   </inertial>
 </xacro:macro>
