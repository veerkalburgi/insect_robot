  <origin xyz="0 0 0" rpy="0 0 0"/>
  <geometry>
    <mesh filename="package://insect_description/meshes/torso_v1.dae"/>
  </geometry>
</visual>
<xacro:default_inertial mass="0.8"/>
</link>

