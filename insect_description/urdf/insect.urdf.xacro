<joint name="world_joint"  type="fixed">
  <origin xyz="0 0 0.30" rpy="0 0 0" />
  <parent link="world"/>
  <child link="torso_base"/>
  <axis xyz="0 1 0" rpy="0 0 0"/>
</joint>
<!--IMU sensor joint-->
<joint name="imu_joint" type="fixed">
  <axis xyz="0.01 0 0.01"/> <!-- 0 1 0 -->
  <origin xyz="0 0 0.05"/>
  <parent link="torso_base"/>
  <child link="imu_link"/>
</joint>

