<link name="world"/>

<link name="torso_base">
<collision name="collision">
  <origin xyz="0 0 0" rpy="0 0 0"/>
  <geometry>
    <mesh filename="package://insect_description/meshes/torso_v1.dae"/>
  </geometry>
</collision>
